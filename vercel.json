{
  "version": 2,
  "buildCommand": "pnpm prisma generate && pnpm tsc",
  "installCommand": "pnpm install",
  "rewrites": [
    {
      "source": "/api/swagger/swagger-ui.css",
      "destination": "https://cdnjs.cloudflare.com/ajax/libs/swagger-ui/5.10.5/swagger-ui.css"
    },
    {
      "source": "/api/swagger/swagger-ui-bundle.js",
      "destination": "https://cdnjs.cloudflare.com/ajax/libs/swagger-ui/5.10.5/swagger-ui-bundle.js"
    },
    {
      "source": "/api/swagger/swagger-ui-standalone-preset.js",
      "destination": "https://cdnjs.cloudflare.com/ajax/libs/swagger-ui/5.10.5/swagger-ui-standalone-preset.js"
    },
    {
      "source": "/api/swagger/favicon-16x16.png",
      "destination": "https://cdnjs.cloudflare.com/ajax/libs/swagger-ui/5.10.5/favicon-16x16.png"
    },
    {
      "source": "/api/swagger/favicon-32x32.png",
      "destination": "https://cdnjs.cloudflare.com/ajax/libs/swagger-ui/5.10.5/favicon-32x32.png"
    },
    {
      "source": "/(.*)",
      "destination": "/index.ts"
    }
  ]
}