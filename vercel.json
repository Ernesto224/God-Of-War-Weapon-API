{
    "version": 2,
    "builds": [
        {
            "src": "server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server.ts"
        },
        {
            "src": "/api/swagger/(.*)",
            "dest": "server.ts"
        }
    ]
}