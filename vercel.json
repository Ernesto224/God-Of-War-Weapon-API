{
    "version": 2,
    "builds": [
        {
            "src": "dist/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/swagger($|/.*)",
            "dest": "dist/server.js",
            "headers": {
                "Cache-Control": "public, max-age=7200, immutable"
            }
        },
        {
            "src": "/api/(.*)",
            "dest": "dist/server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "DATABASE_URL": "***REMOVED***" 
    }
}